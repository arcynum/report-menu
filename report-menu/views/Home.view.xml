<?xml version="1.0"?>
<mvc:View
	xmlns="sap.m"
	xmlns:template="sap.ui.core.template"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	controllerName="report-menu.controllers.Home">

	<Page title="{i18n&gt;home.title}">
		<content>
			<Panel content="{ReportModel&gt;/reports}">
				<content>
					<Panel>
						<headerToolbar>
							<Toolbar>
								<Title text="{ReportModel&gt;title}" />
								<ToolbarSpacer />
								<Button icon="{ReportModel&gt;icon}" />
							</Toolbar>
						</headerToolbar>
						<content>
							<Text text="{ReportModel&gt;description}" />

							<Panel headerText="Wut" content="{ReportModel&gt;sections}">
								<content>
									<Text text="{ReportModel&gt;description}" />
								</content>
							</Panel>

						</content>
					</Panel>
				</content>
			</Panel>
		</content>
	</Page>

</mvc:View>