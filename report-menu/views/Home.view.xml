<?xml version="1.0"?>
<mvc:View
	xmlns="sap.m"
	xmlns:template="sap.ui.core.template"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	controllerName="librarian.controllers.Home">
	<Page title="{i18n&gt;home.title}">
		<content>
			<TileContainer
				id="role-selection"
				tiles="{RoleModel&gt;/roles}">
				<StandardTile
					name="{RoleModel&gt;name}"
					icon="{RoleModel&gt;icon}"
					type="Monitor"
					title="{RoleModel&gt;description}"
					press="onTilePress" />
			</TileContainer>
		</content>
	</Page>
</mvc:View>